{
  "compilerOptions": {
    "outDir": "./dist",
    /* Linting */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    /* Type declarations */
    "sourceMap": true,
    "declaration": true,
    // "declarationDir": "dist",
    "emitDeclarationOnly": true,
    "declarationMap": true
  },
  "include": [
    "src"
  ]
}